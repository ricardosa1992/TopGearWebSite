<body id="top" data-spy="scroll" data-target=".navbar" data-offset="260">
  

    <section class="content">
        <div class="container-fluid">
            <div class="row">
              
                <div class="col-md-10">
                    <ul>
                        <li><h4>Que tal uma passagem Aérea?</h4></li>
                        <li><a href="@Url.Action("LocacoesCliente", "Locacao")"><i class="fa fa-gear fa-fw"></i>Ir para Reservas</a></li>
                    </ul>
                  
                 

                    @{
                        foreach (var voo in Model.listaVoos)
                        {
                            <div class="box-detalhes-reserva col-md-3" style="margin-left:30px;margin-top:30px" >
                                <div class="regiao-slip-reserva ativo" id="slip-reserva-sua-reserva">
                                    <div class="reserva-header voo-header">
                                        <a class="target" href="#" title="sua reserva">
                                            <i class="fa fa-plane" aria-hidden="true"></i> @voo.codigo
                                           
                                        </a>
                                    </div>
                                    <div class="reserva-body">
                                        <i class="icon2-retirada"></i><p class="sub-title">Partida</p>
                                        <div class="content-agencias">
                                            <p class="text-slip-strong" style="text-transform:capitalize">
                                                @voo.cidade_partida - @voo.aeroporto_partida (@voo.sigla_aeroporto_partida)
                                            </p>
                                            <div class="date text-slip-normal">
                                                @voo.partida
                                            </div>
                                        </div>
                                        <p class="sub-title"><i class="icon2-devolucao"></i>Chegada</p>
                                        <div class="content-agencias">
                                            <p class="text-slip-strong" style="text-transform:capitalize">
                                              @voo.cidade_destino - @voo.aeroporto_destino (@voo.sigla_aeroporto_destino)
                                            </p>
                                            <div class="date text-slip-normal">
                                                @voo.chegada
                                            </div>
                                        </div>

                                        <div id="qtdDiarias" class="box-detalhes-reserva" style="">
                                            <p class="sub-title"> <i class="icon2-seu-carro"></i> Preço: <font color="black">R$ @voo.preco.ToString("n2")</font></p>
                                           
                                        </div>
                                        <input type="button" class="btn btn-primary btn-block" id="confirmarLocacao" value="Comprar" style="" />

                                    </div>

                                </div>
                             
                            </div>

                        }


                    }


                </div>

                <!--Area da reserva-->
                <div class="col-md-2">
                <div class="sidenav">
                    <div id="div-fixo-slip" class="col-2 pull-right" style="">

                        <div class="box-detalhes-reserva">
                            <div class="regiao-slip-reserva ativo" id="slip-reserva-sua-reserva">
                                <div class="reserva-header">
                                    <a class="target" href="#" title="sua reserva">
                                        <i class="icon2-sua-reserva"></i><h1 style="margin-top:5%">sua reserva</h1>
                                    </a>
                                </div>
                                <div class="reserva-body">
                                    <i class="icon2-retirada"></i><p class="sub-title">Retirada</p>
                                    <div class="content-agencias">
                                        <p class="text-slip-strong" style="text-transform:capitalize">
                                            @Model.localRetirada.Nome
                                            @Html.HiddenFor(m => m.idLocalRetiradaHidden, new { Value = Model.localRetirada.Id })
                                        </p>
                                        <div class="date text-slip-normal">
                                            @Model.dataRetirada.ToString()
                                            @Html.HiddenFor(m => m.dataRetiradaHidden, new { Value = Model.dataRetirada.ToString() })
                                        </div>
                                    </div>
                                    <p class="sub-title"><i class="icon2-devolucao"></i>Devolução</p>
                                    <div class="content-agencias">
                                        <p class="text-slip-strong" style="text-transform:capitalize">
                                            @Model.localEntrega.Nome
                                            @Html.HiddenFor(m => m.idLocalEntregaHidden, new { Value = Model.localEntrega.Id })
                                        </p>
                                        <div class="date text-slip-normal">
                                            @Model.dataEntrega.ToString()
                                            @Html.HiddenFor(m => m.dataEntregaHidden, new { Value = Model.dataEntrega.ToString() })
                                        </div>
                                    </div>

                                    <div id="qtdDiarias" class="box-detalhes-reserva" style="">
                                        <p class="sub-title"> <i class="icon2-seu-carro"></i> Diárias: <font color="black">@Model.QtdDiarias</font></p>
                                        @Html.HiddenFor(m => m.QtdDiarias, new { Value = Model.QtdDiarias })
                                    </div>

                                    <div id="slip-reserva-upgrade" class="box-detalhes-reserva seu-carro" style="">
                                        <br>
                                        @*<span class="text-slip-normal" id="carroSelecionado"></span>*@
                                        <div class=""><img style="width:250px" id="caminhoFotoCarroSelecionado" src="@Model.carroSelecionado.UrlImagem" alt="" /></div>
                                        <div class="info">
                                            <h4 id="carroSelecionado"><font color="#00984a"> Modelo: </font>@Model.carroSelecionado.Modelo</h4>
                                            @Html.HiddenFor(m => m.idCarroSelecionadoHidden, new { Value = Model.carroSelecionado.Id })
                                        </div>

                                    </div>

                                    <div class="reserva-footer" id="slip-reserva-valor" style="">
                                        <div class="slip-reserva-valor-total">
                                            <h3 class="titulo valor-previsto" id="valorLocacao"><font color="#00984a">Total: </font>@Model.QtdDiarias X @Model.precoDiaria = R$ @Model.precoTotal.ToString("n2")</h3>
                                            @Html.HiddenFor(m => m.precoTotal)
                                        </div>
                                    </div>

                                </div>

                            </div>
                            @*<div class="reserva-footer" id="slip-reserva-valor" style="display:none;">
                                    <div class="slip-reserva-valor-total">
                                        <p class="titulo valor-previsto" id="valorLocacao"></p>
                                    </div>
                                </div>

                                <input type="button" class="btn btn-primary" id="confirmarLocacao" value="Continuar" style="display:none" />*@
                        </div>
                    </div>
                </div>
                </div>
                <!--Fim Area reserva-->

            </div>
        </div>

        

    </section>


    <a href="#" class="scrollup">ScrollUp</a>

